includes:
	- config.level6.neon

parameters:
	checkNullables: true

services:
	-
		factory: PHPStan\Rules\Properties\AccessPropertiesOnPossiblyNullRule
		arguments:
			checkThisOnly: %checkThisOnly%
		tags:
			- phpstan.rules.rule

	-
		factory: PHPStan\Rules\Methods\CallMethodsOnPossiblyNullRule
		arguments:
			checkThisOnly: %checkThisOnly%
		tags:
			- phpstan.rules.rule
