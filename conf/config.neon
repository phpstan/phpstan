parameters:
	bootstrap: null
	excludes_analyse: []
	autoload_directories: []
	autoload_files: []
	fileExtensions:
		- php
	checkFunctionArgumentTypes: false
	checkArgumentsPassedByReference: false
	checkMaybeUndefinedVariables: false
	checkNullables: false
	checkUnionTypes: false
	polluteScopeWithLoopInitialAssignments: false
	polluteCatchScopeWithTryAssignments: false
	ignoreErrors: []
	reportUnmatchedIgnoredErrors: true
	universalObjectCratesClasses:
		- stdClass
		- SimpleXMLElement
	earlyTerminatingMethodCalls: []
	memoryLimitFile: %tmpDir%/.memory_limit

services:
	-
		factory: PhpParser\BuilderFactory

	-
		factory: PhpParser\Lexer

	-
		factory: PhpParser\NodeTraverser
		setup:
			- addVisitor(@PhpParser\NodeVisitor\NameResolver)

	-
		factory: PhpParser\NodeVisitor\NameResolver

	-
		factory: PhpParser\Parser\Php7

	-
		factory: PhpParser\PrettyPrinter\Standard

	-
		factory: PHPStan\Analyser\Analyser
		arguments:
			ignoreErrors: %ignoreErrors%
			reportUnmatchedIgnoredErrors: %reportUnmatchedIgnoredErrors%
			bootstrapFile: %bootstrap%

	-
		factory: PHPStan\Analyser\NodeScopeResolver
		arguments:
			polluteScopeWithLoopInitialAssignments: %polluteScopeWithLoopInitialAssignments%
			polluteCatchScopeWithTryAssignments: %polluteCatchScopeWithTryAssignments%
			earlyTerminatingMethodCalls: %earlyTerminatingMethodCalls%

	-
		factory: PHPStan\Analyser\TypeSpecifier

	-
		factory: PHPStan\Broker\BrokerFactory

	-
		factory: PHPStan\Cache\Cache
		arguments:
			storage: @cacheStorage

	-
		factory: PHPStan\Command\AnalyseApplication
		arguments:
			memoryLimitFile: %memoryLimitFile%
			fileExtensions: %fileExtensions%

	-
		factory: PHPStan\File\FileHelper
		arguments:
			workingDirectory: %currentWorkingDirectory%

	-
		factory: PHPStan\File\FileExcluder
		arguments:
			analyseExcludes: %excludes_analyse%

	-
		factory: PHPStan\Parser\CachedParser
		arguments:
			originalParser: @directParser

	-
		factory: PHPStan\Parser\FunctionCallStatementFinder

	-
		implement: PHPStan\Reflection\FunctionReflectionFactory

	-
		factory: PHPStan\Reflection\Annotations\AnnotationsMethodsClassReflectionExtension

	-
		factory: PHPStan\Reflection\Annotations\AnnotationsPropertiesClassReflectionExtension

	-
		factory: PHPStan\Reflection\Php\PhpClassReflectionExtension

	-
		factory: PHPStan\Reflection\PhpDefect\PhpDefectClassReflectionExtension

	-
		implement: PHPStan\Reflection\Php\PhpMethodReflectionFactory

	-
		factory: PHPStan\Reflection\Php\UniversalObjectCratesClassReflectionExtension
		tags:
			- phpstan.broker.propertiesClassReflectionExtension
		arguments:
			classes: %universalObjectCratesClasses%

	-
		factory: PHPStan\Rules\FunctionCallParametersCheck
		arguments:
			checkArgumentTypes: %checkFunctionArgumentTypes%
			checkArgumentsPassedByReference: %checkArgumentsPassedByReference%

	-
		factory: PHPStan\Rules\FunctionDefinitionCheck

	-
		factory: PHPStan\Rules\FunctionReturnTypeCheck

	-
		factory: PHPStan\Rules\Properties\PropertyDescriptor

	-
		factory: PHPStan\Rules\Properties\PropertyReflectionFinder

	-
		factory: PHPStan\Rules\RegistryFactory

	-
		factory: PHPStan\Rules\RuleLevelHelper
		arguments:
			checkNullables: %checkNullables%

	-
		factory: PHPStan\Rules\UnusedFunctionParametersCheck

	-
		factory: PHPStan\Type\FileTypeMapper

	broker:
		class: PHPStan\Broker\Broker
		factory: @PHPStan\Broker\BrokerFactory::create

	cacheStorage:
		factory: PHPStan\Cache\FileCacheStorage
		arguments:
			directory: %tmpDir%/cache/PHPStan
		autowired: no

	directParser:
		factory: PHPStan\Parser\DirectParser
		autowired: no

	registry:
		class: PHPStan\Rules\Registry
		factory: @PHPStan\Rules\RegistryFactory::create

	errorFormatter.raw:
		factory: PHPStan\Command\ErrorFormatter\RawErrorFormatter

	errorFormatter.table:
		factory: PHPStan\Command\ErrorFormatter\TableErrorFormatter

	errorFormatter.checkstyle:
		factory: PHPStan\Command\ErrorFormatter\CheckstyleErrorFormatter
