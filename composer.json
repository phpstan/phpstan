{
	"name": "phpstan/phpstan",
	"description": "PHPStan - PHP Static Analysis Tool",
	"license": ["MIT"],
	"require": {
		"php": "^7.1"
	},
	"conflict": {
		"phpstan/phpstan-shim": "*"
	},
	"bin": [
		"phpstan",
		"phpstan.phar"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "0.12-dev"
		},
		"thanks": {
			"name": "phpstan/phpstan-src",
			"url": "https://github.com/phpstan/phpstan-src"
		}
	},
	"autoload": {
		"files": ["bootstrap.php"]
	}
}
