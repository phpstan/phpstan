	<script>
		window.ThemeSwitcher = {
			storageKey: 'color-theme',
			reloadTheme: () => {
				let theme = localStorage.getItem(ThemeSwitcher.storageKey) || 'auto';
				if (theme === 'auto') {
					theme = window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light';
				}
				document.documentElement.classList.toggle('dark', theme === 'dark');
				// TODO This is only useful to be caught by other components to do the switch.
				window.dispatchEvent(new CustomEvent('themechange', {detail: {theme}}));
			}
		};
		window.addEventListener('storage', ThemeSwitcher.reloadTheme);
		ThemeSwitcher.reloadTheme();
	</script>
	<script src="/tmp/js/ThemeSwitcher.ts" type="module"></script>
</head>
